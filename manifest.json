{
	"manifest_version": 2,
	"name": "GeoSync",
	"description": "A content tracker that let to detect the score and the order of GeoGuessr rounds",
	"version": "0.1.0",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"cookies",
		"storage"
	],
	"browser_action": {
		"default_icon" : "media/images/favicon.png" // L'icône qui s'affiche en haut à droite de votre navigateur
	},
	"background": {
		"scripts": [
			"js/round_emitter.js"
		],
		"persistent": true
	},
	"content_scripts": [
		{
			"matches": [
			  "https://www.geoguessr.com/*"
			],
			"js": [
			  "js/round_analyzer.js"
			]
		}
	],
	"icons": { // L'îcone qui s'affichera lorsquee que vous êtes sur la page extensions des paramètre
		"128" : "media/images/favicon.png" // 128 = 128x128 la taille de votre image
	}
}